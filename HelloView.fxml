<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.image.ImageView?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="cs112.lab08.HelloController">
    <Label fx:id="titleLabel" text="Welcome to EChALE STEM Loteria!" style="-fx-font-size: 15;"/>
    <ImageView fx:id="cardImage" fitHeight="300" fitWidth="200" preserveRatio="true"/>
    <Label fx:id="cardLabel"/>

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button fx:id= "nextButton" text="Draw Random Card"/>
</VBox>
